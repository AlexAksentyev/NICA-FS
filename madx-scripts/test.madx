call file = "nica_24sol_rbend.seq";

deu_mass = 1.8765592; // GeV

beam, PARTICLE=ION, ENERGY=0.270+deu_mass, MASS=deu_mass;
use, sequence=snake;

twiss, save, centre, file=twiss.out;

plot, haxis=s, vaxis=betx,bety,dx,colour=100;
survey, file=survey.out;

TRACK, file=basis, dump;
START, x=1e-3;
START, y=1e-3;
RUN, turns=10;
ENDTRACK;

PLOT, file=track_plot, table=trackone, trackfile=basis,
      haxis=turn, vaxis=x,y;

stop;

